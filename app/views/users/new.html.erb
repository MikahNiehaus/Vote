<%= form_for @user do |f|%>
    <%= f.label :name %>
    <%= f.text_field :name %>

    <%= f.label :height %> 
    <%= f.number_field :height %>

    <%= f.label :happiness %> 
    <%= f.number_field :happiness %>

    <%= f.label :nausea %>
    <%= f.number_field :nausea %>

    <%= f.label :tickets %>
    <%= f.number_field :tickets %>

    <%= f.label :password %>
    <%= f.password_field :password %>


    <label>Check if you are the owner</label>
    <%= f.check_box :admin %> 

    <%= f.submit "Create User"%>

<% end %>